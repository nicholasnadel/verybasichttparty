<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Avatar url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @response.each do |response| %>
    <li><%= image_tag response["url"] %></li>
    <li><%= response["snippet"] %> </li>
    <% end %>
    
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
